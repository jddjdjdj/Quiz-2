package Semana2;

/**
 *
 * @author pc
 */
public class MiSegundaClase {
    
    public static void main(String[] args){
        
        System.out.println("El resultado es");
        
        int box1;
        int num2;
        int z;
        box1 = 5;
        num2 = 30;
        z = box1 + num2;
        
        //Esta linea imprime al usuario el resultado
        System.out.print(z);
        
    }
}
    
